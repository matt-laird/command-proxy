{
  "name": "command-proxy",
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node --transpile-only src/server.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --delay 1 --exec 'ts-node' src/server.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "hpp": "^0.2.3",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^4.3.1",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.1",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.14.2",
    "nodemon": "^2.0.6",
    "tslint": "^6.1.3"
  }
}
